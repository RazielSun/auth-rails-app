
<!-- Main component for a primary marketing message or call to action -->
<div class="jumbotron">
<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each_with_index do |user, index| %>
    <tr>
      <th scope="row"><%= index+1 %></th>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>